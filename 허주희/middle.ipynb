    "def solution(s):\n",
    "    answer = ''\n",
    "\n",
    "    if len(s) % 2 != 0 :\n",
    "        point = len(s) // 2\n",
    "        answer = s[point]\n",
    "    else :\n",
    "        point = len(s) // 2\n",
    "        answer = s[point-1 : point+1]\n",
    "    return answer\n",
    "\n",
    "s = \"abcdefgh\"\n",
    "print(solution(s))"
   ]
