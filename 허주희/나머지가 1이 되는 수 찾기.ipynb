    "def solution(n):\n",
    "    answer = []\n",
    "    \n",
    "    for q in range(n) :\n",
    "        q += 1\n",
    "        x = 0\n",
    "        for x in range(n) :\n",
    "            x += 1\n",
    "            if q * x == n-1 :\n",
    "                answer.append(x)\n",
    "                break\n",
    "        answer.sort()\n",
    "        \n",
    "    return answer[1]"
   ]
  }
