    "def solution(numbers):\n",
    "    answer = 0\n",
    "    \n",
    "    for n in range(0,10) :\n",
    "        check = 0\n",
    "        for i in numbers:\n",
    "            if n == i :\n",
    "                check += 1\n",
    "        if check == 0 :\n",
    "            answer += n\n",
    "    return answer\n",
    "\n",
    "numbers = [1,2,3,4,6,7,8,0]\n",
    "print(solution(numbers))"
   ]
  }
