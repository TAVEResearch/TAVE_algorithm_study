    "def solution(x, n):\n",
    "    answer = []\n",
    "    x1 = x\n",
    "    \n",
    "    for i in range(n) :\n",
    "        answer.append(x1)\n",
    "        x1 += x\n",
    "    return answer\n",
    "\n",
    "x = 2\n",
    "n = 5\n",
    "print(solution(x, n))"
   ]
